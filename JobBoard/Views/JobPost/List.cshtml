@model JobPostsListViewModel

@foreach (var jp in Model.JobPosts) {
    <div>
        <h3>@jp.Title</h3>
        <p>@jp.PostDate.ToString("MMM/dd/yyyy")</p>
        <p>@jp.City</p>
        <p>@jp.Description</p>
    </div>
}

<div page-model="@Model.PagingInfo" page-action="List" 
     page-classes-enabled="true"
     page-class="btn" page-class-normal="btn-primary"
     page-class-selected="btn-info" class="btn-group pull-right" >
</div>
